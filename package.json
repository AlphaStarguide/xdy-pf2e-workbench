    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "foundry-types": "file:types/types/foundry",
    "fs-extra": "^11.3.0",
    "globals": "^16.3.0",
    "gsap": "^3.13.0",
    "handlebars": "4.7.8",
    "jquery": "^3.7.1",
    "jsdom": "^26.1.0",
    "peggy": "^4.2.0",
    "pixi.js": "^7.4.3",
